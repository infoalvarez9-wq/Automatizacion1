import numpy as np
def matriz_transpuesta():
    n = int(input("Defina el tamaño de la matriz (n): "))
    conjunto = []
    for i in range(n):
        fila = []
        for j in range(n):
            valor = float(input(f"Ingrese el valor para la posición ({i},{j}): "))
            fila.append(valor)
        conjunto.append(fila)
    matriz = np.array(conjunto)
    transpuesta = matriz.T
    print("\nMatriz original:")
    print(matriz)
    print("\nMatriz transpuesta:")
    print(transpuesta)
matriz_transpuesta()